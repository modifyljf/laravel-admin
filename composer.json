{
  "name": "modifyljf/laravel-admin",
  "description": "Laravel admin wrote by Jianfeng Li.",
  "keywords": [
    "admin",
    "laravel"
  ],
  "license": "MIT",
  "homepage": "https://github.com/modifyljf/laravel-admin",
  "support": {
    "issues": "https://github.com/modifyljf/laravel-admin/issues",
    "source": "https://github.com/modifyljf/laravel-admin"
  },
  "authors": [
    {
      "name": "Jianfeng Li",
      "email": "modifyljf@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "facade/ignition": "^2.3.8",
    "fzaninotto/faker": "^1.9.2",
    "laravel/breeze": "^1.9",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^v5.0.0",
    "phpunit/phpunit": "^9.4.0"
  },
  "autoload": {
    "psr-4": {
      "Modifyljf\\Admin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Modifyljf\\Admin\\Providers\\BeanServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.0"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
